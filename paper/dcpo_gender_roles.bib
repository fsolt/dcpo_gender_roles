	Journal = {American Journal of Political Science},
	Number = {2},
	Pages = {355-368},
	Title = {Defending the Realm: The Appointment of Female Defense Ministers Worldwide},
	Volume = {62},
	Year = {2018},
	Bdsk-File-1 = {},
